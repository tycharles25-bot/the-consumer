datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model WebhookEvent {
  id          String   @id @default(cuid())
  source      String
  topic       String?
  objectId    String?
  payload     Json
  receivedAt  DateTime @default(now())
}

model MetaCredential {
  id           String   @id @default(cuid())
  type         String
  value        String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
